  m_SharedData: {fileID: 11400000, guid: 30630d9d68e57f84bb4a6bc4784ad227, type: 2}
  m_Metadata:
    m_Items: []
  m_TableData:
  - m_Id: 58963357696
    m_Localized: "<b>Bei McDonald\u2019s f\xF6rdern und erhalten wir einen sicheren,
      respektvollen und inklusiven Arbeitsplatz f\xFCr alle:</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 32415523106816
    m_Localized: Die Aufgabe eines:einer Manager:in bei der Schaffung eines sicheren,
      respektvollen und inklusiven Arbeitsplatzes
    m_Metadata:
      m_Items: []
  - m_Id: 280336076206080
    m_Localized: <size=+20><b>Die Aufgabe eines:einer Manager:in bei der Schaffung
      eines sicheren, respektvollen und inklusiven Arbeitsplatzes</b>
    m_Metadata:
      m_Items: []
  - m_Id: 300762370240512
    m_Localized: "T\xE4glich unsere Werte leben und vertreten"
    m_Metadata:
      m_Items: []
  - m_Id: 301541340573696
    m_Localized: Jeden mit Respekt behandeln
    m_Metadata:
      m_Items: []
  - m_Id: 324650974146560
    m_Localized: "Sich der Auswirkungen unserer Worte und Handlungen auf andere bewusst
      sein und dar\xFCber nachdenken"
    m_Metadata:
      m_Items: []
  - m_Id: 328896373157888
    m_Localized: Es ansprechen, wenn Sie unangemessenes Verhalten mitbekommen oder
      erleben
    m_Metadata:
      m_Items: []
  - m_Id: 329567763787776
    m_Localized: Allen, insbesondere den Manager:innen, kommen wichtige Funktionen
      zu, um eine respektvolle Unternehmenskultur zu schaffen
    m_Metadata:
      m_Items: []
  - m_Id: 2592079758446592
    m_Localized: "Als Manager:in \xFCbernehmen Sie eine entscheidende Funktion beim
      Aufbau eines sicheren, respektvollen und inklusiven Arbeitsplatzes.\n\nEs wird
      von Ihnen erwartet, dass Sie ein Vorbild f\xFCr Ihr Team sind und stets mit
      gutem Beispiel vorangehen!"
    m_Metadata:
      m_Items: []
  - m_Id: 2592417236340736
    m_Localized: "<b>Manager:innen \xFCbernehmen als Vorbilder eine besondere Verantwortung,
      dazu geh\xF6rt Folgendes:</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 2592760418488320
    m_Localized: "Geben Sie ein positives Beispiel, indem Sie sicherstellen, dass
      Ihr Verhalten, einschlie\xDFlich Ihrer Besch\xE4ftigungsentscheidungen und
      Ihres Verhaltens am Arbeitsplatz, frei von unangemessenen Verhaltensweisen
      wie Bel\xE4stigung (physisch, psychisch, sexuell), Diskriminierung, Mobbing
      und Vergeltung/Viktimisierung ist."
    m_Metadata:
      m_Items: []
  - m_Id: 2593642577088512
    m_Localized: "Ma\xDFnahmen zur Beseitigung aller unangemessenen Verhaltensweisen
      wie Bel\xE4stigung (physisch, psychisch, sexuell), Diskriminierung, Mobbing
      und Vergeltung/Viktimisierung."
    m_Metadata:
      m_Items: []
  - m_Id: 2594270552477696
    m_Localized: "Eskalieren Sie sofort, wenn Sie unangemessene Verhaltensweisen
      mitbekommen oder wenn Mitarbeiter:innen Sie auf Verhaltensweisen aufmerksam
      machen, durch die sie oder Andere sich unwohl oder unsicher f\xFChlen."
    m_Metadata:
      m_Items: []
  - m_Id: 2594492410187776
    m_Localized: "Nehmen Sie Anschuldigungen oder Berichte \xFCber alle unangemessenen
      Verhaltensweisen wie Bel\xE4stigung (k\xF6rperlich, psychologisch, sexuell),
      Diskriminierung, Mobbing und Benachteiligung/Schikane ernst."
    m_Metadata:
      m_Items: []
  - m_Id: 2595063053635584
    m_Localized: "Zusammenarbeit mit den richtigen Personen, um Anschuldigungen unverz\xFCglich
      und gr\xFCndlich zu untersuchen."
    m_Metadata:
      m_Items: []
  - m_Id: 2595208877002752
    m_Localized: "Stellen Sie sicher, dass Sie und Ihre Mitarbeiter:innen wissen,
      wie und wo sie sich melden k\xF6nnen, wenn sie unangemessenes Verhalten erleben
      oder erkennen."
    m_Metadata:
      m_Items: []
  - m_Id: 2609365743521792
    m_Localized: <b>Was sollten Sie in dieser Situation Ihrer Meinung nach tun?</b>
    m_Metadata:
      m_Items: []
  - m_Id: 2609551664435200
    m_Localized: "<b>Jemand hat sich mit einer m\xF6glichen Beschwerde an Sie gewandt.\nWas
      sollten Sie in dieser Situation Ihrer Meinung nach tun?</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 2609684217024512
    m_Localized: "Drehen Sie die Karten um, um mehr \xFCber den Zusammenhang zu erfahren,
      und w\xE4hlen Sie dann alle zutreffenden aus."
    m_Metadata:
      m_Items: []
  - m_Id: 2610809649451008
    m_Localized: Danken Sie der Person, dass sie sich gemeldet hat.
    m_Metadata:
      m_Items: []
  - m_Id: 2610878549282816
    m_Localized: Nehmen Sie die Beschwerde ernst.
    m_Metadata:
      m_Items: []
  - m_Id: 2610915408826368
    m_Localized: "H\xF6ren Sie zu, aber ergreifen Sie nicht Partei und ziehen Sie
      keine Schl\xFCsse."
    m_Metadata:
      m_Items: []
  - m_Id: 2610968991059968
    m_Localized: "Erkl\xE4ren Sie, dass dem nachgegangen wird."
    m_Metadata:
      m_Items: []
  - m_Id: 2611299066007552
    m_Localized: "Eine junge Mitarbeiterin hat Sie dar\xFCber informiert, dass ihr
      ein Lieferant zugezwinkert hat und sie sich dabei unwohl gef\xFChlt hat.\n\nIst
      die folgende Antwort der:des Manager:in angemessen?"
    m_Metadata:
      m_Items: []
  - m_Id: 2611431417270272
    m_Localized: Adam
    m_Metadata:
      m_Items: []
  - m_Id: 2611612242104320
    m_Localized: "\u201EDer Lieferant zwinkert jedem zu, das ist harmlos.\u201C\nWenn
      sie etwas Schlimmeres tun, k\xF6nnen Sie sich jederzeit an uns wenden und uns
      Bescheid geben."
    m_Metadata:
      m_Items: []
  - m_Id: 2611893193363456
    m_Localized: "Richtig! Manager sollten potenziell unangemessenes Verhalten immer
      ernst nehmen. Dies bedeutet, dass der Vorfall unverz\xFCglich gemeldet werden
      muss."
    m_Metadata:
      m_Items: []
  - m_Id: 2612031949328384
    m_Localized: "Falsch, Manager:innen sollten potenziell unangemessenes Verhalten
      immer ernst nehmen. Dies bedeutet, dass der Vorfall unverz\xFCglich gemeldet
      werden muss."
    m_Metadata:
      m_Items: []
  - m_Id: 2612287864786944
    m_Localized: "Was sollten Sie tun, wenn Mitarbeiter:innen Ihnen mitteilen, dass
      sie Zeug:innen unangemessener \xC4u\xDFerungen eines Senior Leaders gegen\xFCber
      Mitarbeiter:innen wurden?"
    m_Metadata:
      m_Items: []
  - m_Id: 2612453514629120
    m_Localized: Warten Sie ab, und beobachten Sie, ob sich das Verhalten des Senior
      Leaders wiederholt.
    m_Metadata:
      m_Items: []
  - m_Id: 2612574507716608
    m_Localized: "Erkl\xE4ren Sie den Mitarbeiter:innen, dass so etwas manchmal vorkommt
      und keine gro\xDFe Sache ist."
    m_Metadata:
      m_Items: []
  - m_Id: 2612620355653632
    m_Localized: "Erkl\xE4ren Sie, dass das Verhalten des Senior Leaders nicht toleriert
      wird und melden Sie es."
    m_Metadata:
      m_Items: []
  - m_Id: 2612676479635456
    m_Localized: "Richtig! Als Manager:in liegt es in Ihrem Ermessen, unangemessenes
      Verhalten zu unterbinden. Jegliches unangemessene Verhalten, einschlie\xDFlich
      des Verhaltens von Senior Leader:innen, muss gemeldet und ernst genommen werden."
    m_Metadata:
      m_Items: []
  - m_Id: 2612770310410240
    m_Localized: "Falsch, als Manager:in liegt es in Ihrem Ermessen, unangemessenes
      Verhalten zu unterbinden. Jegliches unangemessene Verhalten, einschlie\xDFlich
      des Verhaltens von Senior Leader:innen, muss gemeldet und ernst genommen werden."
    m_Metadata:
      m_Items: []
  - m_Id: 2612922806915072
    m_Localized: "Ein Mitarbeiter beschwert sich bei Ihnen, dass er von einem Manager
      bel\xE4stigt wurde, weil er zum zweiten Mal in dieser Woche zu sp\xE4t von
      einer Pause zur\xFCckgekommen ist. Der Manager nahm den Mitarbeiter zur Seite,
      um Problem zu besprechen und zu erreichen, dass sich der Mitarbeiter zur P\xFCnktlichkeit
      verpflichtet.\n\_\nIst das Bel\xE4stigung?"
    m_Metadata:
      m_Items: []
  - m_Id: 2613051878232064
    m_Localized: "Richtig! Die Erwartungen von McDonald's mitzuteilen und ein Problem
      mit einem Mitarbeiter einzeln zu besprechen, ist keine Form der Bel\xE4stigung.
      Allerdings k\xF6nnte es sich um eine Bel\xE4stigung handeln, wenn das Feedback
      in einer eindringlichen, verletzenden, bedrohlichen oder einsch\xFCchternden
      Weise gegeben wird. Wenn Sie sich unsicher sind, sollten Sie dies \xFCber den
      richtigen Weg melden."
    m_Metadata:
      m_Items: []
  - m_Id: 2613344506433536
    m_Localized: "Falsch, die Erwartungen von McDonald's mitzuteilen und ein Problem
      mit einem Mitarbeiter einzeln zu besprechen, ist keine Form der Bel\xE4stigung.
      Es k\xF6nnte jedoch eine Bel\xE4stigung darstellen, wenn das Feedback in einer
      schikan\xF6sen Art und Weise gegeben wird. Wenn Sie sich unsicher sind, sollten
      Sie dies \xFCber den richtigen Weg melden."
    m_Metadata:
      m_Items: []
  - m_Id: 2613459648466944
    m_Localized: <b>Verantwortlichkeiten von Manager:innen im Zusammenhang mit Benachteiligung/Schikane</b>
    m_Metadata:
      m_Items: []
  - m_Id: 2613561855266816
    m_Localized: "Vergeltung/Viktimisierung liegt vor, wenn jemand bestraft wird
      oder eine negative arbeitsrechtliche oder individuelle Ma\xDFnahme erf\xE4hrt,
      weil er beabsichtigt, eine Beschwerde einzureichen oder an einer Beschwerde
      (z. B. als Zeuge) \xFCber ein Verhalten am Arbeitsplatz beteiligt ist, wie
      z. B. potenzielle Bel\xE4stigung (k\xF6rperlich, psychologisch, sexuell), Diskriminierung,
      Mobbing oder Vergeltung/Viktimisierung."
    m_Metadata:
      m_Items: []
  - m_Id: 2613610907652096
    m_Localized: "<b>Beispiele f\xFCr Vergeltungsma\xDFnahmen/Viktimisierung:</b>\n\n\u2022<indent=5%>K\xFCndigung</indent>\_\n\u2022<indent=5%>Herabstufung\_</indent>\_\n\u2022<indent=5%>Schlechte
      Leistungsbeurteilung\_\_</indent>\n\u2022<indent=5%>Schichten wechseln</indent>\n\u2022<indent=5%>Ausschluss
      von Teams, Veranstaltungen, Treffen\_</indent>\n\u2022<indent=5%>Unerw\xFCnschte
      Aufgaben\_\_</indent>"
    m_Metadata:
      m_Items: []
  - m_Id: 2613718365720576
    m_Localized: "Als Manager:in sollten Sie jegliches benachteiligende Verhalten
      in Ihrem Team verhindern, unterbinden und melden. Wenn Sie als Manager:in der
      Benachteiligung/Schikane beschuldigt werden, sollten Sie den:die Mitarbeiter:in
      weiterhin vorbildlich behandeln. Kooperieren Sie uneingeschr\xE4nkt und ehrlich
      mit den Pr\xFCfer:innen, und lassen Sie dem Prozess seinen Lauf."
    m_Metadata:
      m_Items: []
  - m_Id: 2613822434791424
    m_Localized: Als Manager:in sollten Sie jegliches benachteiligende Verhalten
      in Ihrem Team verhindern, unterbinden und melden.
    m_Metadata:
      m_Items: []
  - m_Id: 2613934519177216
    m_Localized: "Im folgenden Szenario meldet Samuel seinen Kollegen Miguel wegen
      unangemessenen Verhaltens. Nun sprechen einige von Samuels Mitarbeitern nicht
      mit ihm. Samuel hat seine Kolleg:innen damit konfrontiert, warum er Miguel
      melden musste, aber sie ignorieren ihn noch immer.\_\_\n\_\nAls Anna, die Managerin,
      die Anspannung bemerkt, setzt sie sich zu einem Gespr\xE4ch mit Samuel zusammen."
    m_Metadata:
      m_Items: []
  - m_Id: 2614011002310656
    m_Localized: Anna
    m_Metadata:
      m_Items: []
  - m_Id: 2614046637117440
    m_Localized: Ihr wart alle Freunde, und die Stimmung hier war fantastisch.
    m_Metadata:
      m_Items: []
  - m_Id: 2614120544948224
    m_Localized: Samuel
    m_Metadata:
      m_Items: []
  - m_Id: 2614198881964032
    m_Localized: "Ja, die Dinge \xE4ndern sich."
    m_Metadata:
      m_Items: []
  - m_Id: 2614224014233600
    m_Localized: Kann Anna in dieser Situation Samuel in ein neues Restaurant versetzen?
    m_Metadata:
      m_Items: []
  - m_Id: 2614313474543616
    m_Localized: "Richtig! Es ist klar, dass Anna Samuel aufgrund der Folgen von
      Samuels Beschwerde in ein anderes Restaurant versetzen will. Eine \xC4nderung
      der Arbeitssituation eines Mitarbeiters aufgrund einer Beschwerde ist \xE4u\xDFerst
      riskant, selbst wenn die Managerin und der Mitarbeiter dies f\xFCr positiv
      halten.\n\_\nEine gute Faustregel ist, einen Mitarbeiter so zu behandeln, als
      h\xE4tte er nie eine Beschwerde eingereicht."
    m_Metadata:
      m_Items: []
  - m_Id: 2614433234505728
    m_Localized: "Falsch, es ist klar, dass Anna Samuel aufgrund der Folgen von Samuels
      Beschwerde in ein anderes Restaurant versetzen will. Eine \xC4nderung der Arbeitssituation
      eines Mitarbeiters aufgrund einer Beschwerde ist \xE4u\xDFerst riskant, selbst
      wenn die Managerin und der Mitarbeiter dies f\xFCr positiv halten.\n\_\nEine
      gute Faustregel ist, einen Mitarbeiter so zu behandeln, als h\xE4tte er nie
      eine Beschwerde eingereicht."
    m_Metadata:
      m_Items: []
  - m_Id: 2614603657465856
    m_Localized: "Es kann f\xFCr Mitarbeiter:innen schwierig sein, eine Beschwerde
      einzureichen. Erkennen Sie diese Schwierigkeiten an, indem Sie ihnen danken."
    m_Metadata:
      m_Items: []
  - m_Id: 2615178197422080
    m_Localized: "Es gibt viele Gr\xFCnde, warum Mitarbeiter:innen keine Beschwerde
      einreichen (z.\_B. die Bef\xFCrchtung, dass ihre Beschwerde nicht ernst genommen
      wird)."
    m_Metadata:
      m_Items: []
  - m_Id: 2615219234492416
    m_Localized: "Sammeln Sie nur die Informationen, die erforderlich sind, um den
      Bericht zu erstellen, und gehen Sie dabei mit Mitgef\xFChl und Objektivit\xE4t
      vor."
    m_Metadata:
      m_Items: []
  - m_Id: 2615270258200576
    m_Localized: "Die Festlegung von Erwartungen w\xE4hrend des Berichtsprozesses
      ist entscheidend."
    m_Metadata:
      m_Items: []
  - m_Id: 2616712327024640
    m_Localized: "Jemand, der eine Beschwerde einreicht oder an der Untersuchung
      einer Beschwerde beteiligt ist, kann sich oft unsicher und sogar bedroht f\xFChlen."
    m_Metadata:
      m_Items: []
  - m_Id: 2616952182493184
    m_Localized: "Wenn wir f\xFCreinander eintreten, stehen die Menschen an erster
      Stelle, wir schaffen Inklusion und tun somit das Richtige.\_\_"
    m_Metadata:
      m_Items: []
  - m_Id: 2616995702591488
    m_Localized: "<b>Wann Sie sich offen \xE4u\xDFern sollten</b>\n\_\nSie sind sich
      nicht sicher, wann es das Richtige ist?\_\n\_\nStellen Sie sich diese Fragen
      zur Situation:"
    m_Metadata:
      m_Items: []
  - m_Id: 2617243074252800
    m_Localized: "F\xFChle ich mich durch das, was ich sehe, verletzt oder beleidigt?"
    m_Metadata:
      m_Items: []
  - m_Id: 2617462226636800
    m_Localized: "K\xF6nnte dies gegen unsere Werte versto\xDFen?"
    m_Metadata:
      m_Items: []
  - m_Id: 2617526504345600
    m_Localized: "Hat es den Anschein, dass sich die Person unwohl f\xFChlt?\_"
    m_Metadata:
      m_Items: []
  - m_Id: 2617569235914752
    m_Localized: "F\xFChle ich mich unwohl, wenn ich das sehe?"
    m_Metadata:
      m_Items: []
  - m_Id: 2617613712314368
    m_Localized: "Wenn Sie eine dieser Fragen mit \u201EJa\u201C beantworten, sollten
      Sie sich offen dazu \xE4u\xDFern."
    m_Metadata:
      m_Items: []
  - m_Id: 2617686349271040
    m_Localized: "<b>Es gibt vier M\xF6glichkeiten, sich offen zu \xE4u\xDFern. Jede:r
      kann diese vier M\xF6glichkeiten verwenden, je nach Situation und abh\xE4ngig
      davon, wie wohl er:sie sich f\xFChlt.\n\nDrehen Sie jede Karte um, um weitere
      Informationen zu erhalten.</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 2617762022903808
    m_Localized: "<b>Seien Sie direkt:</b> \nBestimmen Sie die richtige Handlung
      oder das richtige Verhalten.\_"
    m_Metadata:
      m_Items: []
  - m_Id: 2617816230088704
    m_Localized: "<b>Umleiten:</b> \nSprechen Sie ein neues Thema an, und lenken
      Sie das Gespr\xE4ch um."
    m_Metadata:
      m_Items: []
  - m_Id: 2617852200439808
    m_Localized: "<b>Verz\xF6gerung:</b>\nSprechen Sie das Verhalten oder das Unbehagen
      zu einem neutraleren Zeitpunkt an."
    m_Metadata:
      m_Items: []
  - m_Id: 2617899503800320
    m_Localized: "<b>Unterst\xFCtzung suchen:</b>\nAls Manager:in k\xF6nnen Sie sich
      beraten lassen.\_"
    m_Metadata:
      m_Items: []
  - m_Id: 2617962007318528
    m_Localized: "<b>Was sollten Sie tun, wenn Sie Zeuge eines unangemessenen Verhaltens
      eines Kunden gegen\xFCber einem Mitarbeiter werden?</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 2618062515425280
    m_Localized: "Den G\xE4sten sagen, dass wir ihren Besuch sehr sch\xE4tzen, dass
      sie aber wissen sollten, dass hier kein Platz f\xFCr unangemessene Sprache
      ist."
    m_Metadata:
      m_Items: []
  - m_Id: 2618343659622400
    m_Localized: In alleiniger Verantwortung sicherstellen, dass die Bestellung des
      Gastes vorbereitet wird.
    m_Metadata:
      m_Items: []
  - m_Id: 2618387137777664
    m_Localized: "Dem:der Mitarbeiter:in bei der ersten Gelegenheit sagen, dass ein
      solches Verhalten der G\xE4ste hier nicht toleriert wird und das Verhalten
      melden."
    m_Metadata:
      m_Items: []
  - m_Id: 2618432058773504
    m_Localized: "Warten Sie mit der Erw\xE4hnung dessen, was Sie beobachtet haben,
      bis es g\xFCnstiger ist."
    m_Metadata:
      m_Items: []
  - m_Id: 2618481622863872
    m_Localized: Als Manager:in liegt es in Ihrem Ermessen, unangemessenes Verhalten
      zu unterbinden. Jedes unangemessene Verhalten muss gemeldet und ernst genommen
      werden.
    m_Metadata:
      m_Items: []
  - m_Id: 2618524257964032
    m_Localized: Falsch, als Manager:in liegt es in Ihrem Ermessen, unangemessenes
      Verhalten zu unterbinden. Jedes unangemessene Verhalten muss gemeldet und ernst
      genommen werden.
    m_Metadata:
      m_Items: []
  - m_Id: 2618619523190784
    m_Localized: "<b>Warum sollten Sie sich offen \xE4u\xDFern?</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 2618729158103040
    m_Localized: "Sich zu Wort zu melden, entspricht unseren Werten:\n\n\u2022<indent=5%>Menschen
      werden dadurch an die erste Stelle gestellt</indent>\n-<indent=5%>Schafft Inklusion</indent>\n-<indent=5%>Unsere
      Gemeinschaft/McFamily wird gest\xE4rkt</indent>\n\nEs ist die richtige Verhaltensweise."
    m_Metadata:
      m_Items: []
  - m_Id: 2618858913091584
    m_Localized: "Denken Sie an Folgendes, wenn Sie sich offen \xE4u\xDFern:\_\n\n-<indent=5%>Machen
      Sie den ersten Schritt, um ein unangemessenes Verhalten zu beenden</indent>\n-<indent=5%>Agieren
      Sie als Vorbild f\xFCr unsere Werte </indent>\n-<indent=5%>Andere dazu ermutigen,
      ihre Bedenken vorzubringen</indent>"
    m_Metadata:
      m_Items: []
  - m_Id: 2618964798296064
    m_Localized: "\xC4u\xDFern Sie sich offen, um eine Kultur der Zugeh\xF6rigkeit
      zu schaffen, in der alle eine Stimme und vertrauensw\xFCrdige Verb\xFCndete
      haben."
    m_Metadata:
      m_Items: []
  - m_Id: 2619046234902528
    m_Localized: <b>Interaktion mit Mitarbeitern</b>
    m_Metadata:
      m_Items: []
  - m_Id: 2619095698329600
    m_Localized: "\u2022  F\xFCr Manager:innen kann es ein Risiko darstellen, au\xDFerhalb
      der Arbeit privat Zeit mit Mitarbeiter:innen zu verbringen. Oft beginnen diese
      Kontakte positiv, sollten Sie jedoch zu vertraut miteinander werden, kann das
      Ihr Urteilsverm\xF6gen beeintr\xE4chtigen.\n\n\u2022  Wenn Sie Mitarbeiter:innen
      unterschiedlich behandeln, laufen Sie Gefahr, eine Umgebung zu schaffen, die
      als diskriminierend empfunden werden kann. Das kann ein feindliches Arbeitsumfeld
      f\xFCr diejenigen schaffen, die sich nicht bevorzugt f\xFChlen.\n\_\n- Bevorzugung
      bedeutet, dass ein oder mehrere Mitarbeiter bevorzugt behandelt werden, wodurch
      andere auf m\xF6gliche Bef\xF6rderungen oder Vorteile wie bessere Schichten
      verzichten m\xFCssen.\n\n"
    m_Metadata:
      m_Items: []
  - m_Id: 2619130938871808
    m_Localized: "Wenn Sie Mitarbeiter:innen unterschiedlich behandeln, laufen Sie
      Gefahr, eine Umgebung zu schaffen, die als diskriminierend empfunden werden
      kann. Das kann ein feindliches Arbeitsumfeld f\xFCr diejenigen schaffen, die
      sich nicht bevorzugt f\xFChlen.\n\_\nBevorzugung bedeutet, dass ein oder mehrere
      Mitarbeiter bevorzugt behandelt werden, wodurch andere auf m\xF6gliche Bef\xF6rderungen
      oder Vorteile wie bessere Schichten verzichten m\xFCssen."
    m_Metadata:
      m_Items: []
  - m_Id: 2619167324459008
    m_Localized: "Seien Sie sich stets bewusst, dass Handlungen und Worte, auch au\xDFerhalb
      des Arbeitsplatzes, sp\xE4ter Auswirkungen auf das Arbeitsumfeld haben k\xF6nnen."
    m_Metadata:
      m_Items: []
  - m_Id: 2619225725947904
    m_Localized: "- Seien Sie sich stets bewusst, dass Handlungen und Worte, auch
      au\xDFerhalb des Arbeitsplatzes, sp\xE4ter Auswirkungen auf das Arbeitsumfeld
      haben k\xF6nnen.\n\n\u2022  Pr\xFCfen Sie die Richtlinien an Ihrem Arbeitsplatz,
      die den Umgang von Manager:innen mit Mitarbeiter:innen au\xDFerhalb der Arbeit
      betreffen, und erkennen Sie diese an.\n\n- Denken Sie dar\xFCber nach, bevor
      Sie sich au\xDFerhalb der Arbeit mit anderen Mitarbeitern treffen."
    m_Metadata:
      m_Items: []
  - m_Id: 2619266410696704
    m_Localized: "Denken Sie dar\xFCber nach, bevor Sie sich au\xDFerhalb der Arbeit
      mit anderen Mitarbeitern treffen."
    m_Metadata:
      m_Items: []
  - m_Id: 2619331070087168
    m_Localized: "Seien Sie achtsam, wenn Sie mit anderen Mitarbeitern \xFCber Textnachrichten
      oder andere Kan\xE4le der sozialen Medien kommunizieren."
    m_Metadata:
      m_Items: []
  - m_Id: 2619509646774272
    m_Localized: "Jede Interaktion mit Kollegen \xFCber Textnachrichten oder soziale
      Medien sollte respektvoll und professionell sein."
    m_Metadata:
      m_Items: []
  - m_Id: 2619554618101760
    m_Localized: "Vermeiden Sie Beitr\xE4ge, die als b\xF6sartig, obsz\xF6n, bedrohlich
      oder einsch\xFCchternd angesehen werden k\xF6nnten."
    m_Metadata:
      m_Items: []
  - m_Id: 2619588357083136
    m_Localized: "Ver\xF6ffentlichen oder unterst\xFCtzen Sie keine Beitr\xE4ge,
      die diskriminierende \xC4u\xDFerungen oder Inhalte, Bel\xE4stigungen (physisch,
      psychisch, sexuell), Gewaltandrohungen oder \xE4hnliches unangemessenes oder
      rechtswidriges Verhalten enthalten."
    m_Metadata:
      m_Items: []
  - m_Id: 2619636767739904
    m_Localized: "Manager m\xFCssen Vorsicht walten lassen und ein gesundes Urteilsverm\xF6gen
      haben, wenn sie mit Mitarbeitern \xFCber Textnachrichten oder soziale Medien
      kommunizieren."
    m_Metadata:
      m_Items: []
  - m_Id: 2619668753502208
    m_Localized: "Die Teilnahme an solchen Foren mit Untergebenen kann das Potenzial
      erh\xF6hen, unsere Werte zu verletzen. Denken Sie nach, bevor Sie etwas ver\xF6ffentlichen,
      und fragen Sie immer nach Zustimmung."
    m_Metadata:
      m_Items: []
  - m_Id: 2619733047988224
    m_Localized: "Vermeiden Sie au\xDFerdem die Interaktion \xFCber soziale Medien
      mit Mitarbeitern unter 18 Jahren."
    m_Metadata:
      m_Items: []
  - m_Id: 2619797409583104
    m_Localized: "<b>Missbrauch von Autorit\xE4t</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 2619884999233536
    m_Localized: "\u2022  Ein Beispiel f\xFCr den Missbrauch von Autorit\xE4t ist
      ein Fehlverhalten, bei dem eine Person mit Autorit\xE4t (z. B. als Manager:in,
      Vorgesetzte:r oder eine andere Person mit F\xFChrungskompetenz) Versprechungen
      macht, berufliche Vorteile verweigert oder dies androht oder Entscheidungen
      im Austausch f\xFCr sexuelle Gef\xE4lligkeiten trifft.\n\n- Solche Verhaltensweisen
      verletzen unsere Werte und f\xFCgen allen Beteiligten unglaublichen Schaden
      zu."
    m_Metadata:
      m_Items: []
  - m_Id: 2619924635406336
    m_Localized: "Solche Verhaltensweisen versto\xDFen gegen unsere Werte und f\xFCgen
      allen Beteiligten unglaublichen Schaden zu."
    m_Metadata:
      m_Items: []
  - m_Id: 2619964825227264
    m_Localized: "- Der Missbrauch von Autorit\xE4t beschr\xE4nkt sich nicht auf
      den Austausch von Gef\xE4lligkeiten gegen Sex. Jede Aktivit\xE4t, die als Gegenleistung
      f\xFCr einen Gefallen versprochen wird (z. B. ein Date, ein Kuss oder eine
      Umarmung), ist unangemessen und wird von McDonald's nicht toleriert.\n\n- Amtsmissbrauch
      wird nicht nach dem Geschlecht des T\xE4ters oder des Opfers kategorisiert.
      Unabh\xE4ngig von den beteiligten Geschlechtern ist es absolut inakzeptabel,
      jemanden sexuell zu bel\xE4stigen oder die eigene Autorit\xE4t zu missbrauchen."
    m_Metadata:
      m_Items: []
  - m_Id: 2620003890974720
    m_Localized: "Amtsmissbrauch wird nicht nach dem Geschlecht des T\xE4ters oder
      des Opfers kategorisiert. Unabh\xE4ngig von den beteiligten Geschlechtern ist
      es absolut inakzeptabel, jemanden sexuell zu bel\xE4stigen oder die eigene
      Autorit\xE4t zu missbrauchen."
    m_Metadata:
      m_Items: []
  - m_Id: 2620054579138560
    m_Localized: "<b>Es handelt sich auch um Missbrauch von Autorit\xE4t, wenn jemand
      schlechter behandelt wird, weil die Person Angebote f\xFCr sexuelle Aktivit\xE4ten
      oder Gef\xE4lligkeiten ablehnt.</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 2620149190053888
    m_Localized: "<b>Bedrohungen</b>\n\n\u2022   K\xFCndigung\n- Herabstufung\_\n\u2022  
      Schlechte Leistungsbeurteilung\_\n\u2022   Schichten wechseln\n- Ausschluss
      von Teams, Veranstaltungen, Treffen\n- Unerw\xFCnschte Aufgaben\n- Delegieren
      der gleichen Aufgaben (Wischen, Putzen) \n<indent=5%>nur an bestimmte Mitarbeiter:innen"
    m_Metadata:
      m_Items: []
  - m_Id: 2620257784778752
    m_Localized: "<b>Versprechen</b>\n\n\u2022   Weiterbesch\xE4ftigung\_\n\u2022  
      Bef\xF6rderung\_\n\u2022   Positive Leistungsbeurteilung\_\n- Zus\xE4tzliche
      \xDCberstunden"
    m_Metadata:
      m_Items: []
  - m_Id: 2620348922810368
    m_Localized: '<b>Zusammenfassung


      In Ihrer Rolle haben Sie folgende Verantwortung:</b>'
    m_Metadata:
      m_Items: []
  - m_Id: 2620484646293504
    m_Localized: -<indent=5%>Melden Sie jedes unangemessene Verhalten.</indent>
    m_Metadata:
      m_Items: []
  - m_Id: 2620514086113280
    m_Localized: -<indent=5%>Sprechen Sie es an, wenn Sie unangemessenes Verhalten
      mitbekommen.</indent>
    m_Metadata:
      m_Items: []
  - m_Id: 2620555144155136
    m_Localized: "-<indent=5%>Unterst\xFCtzen Sie Ihre Mitarbeiter:innen dabei, sich
      zu \xE4u\xDFern, wenn sie unangemessenes Verhalten sehen oder erleben.</indent>"
    m_Metadata:
      m_Items: []
  - m_Id: 2620572621819904
    m_Localized: -<indent=5%>Verhindern oder stoppen Sie alle Formen von Benachteiligung/Schikane.</indent>
    m_Metadata:
      m_Items: []
  - m_Id: 2620609611386880
    m_Localized: "-<indent=5%>Halten Sie inne und denken Sie nach, bevor Sie sich
      au\xDFerhalb der Arbeit mit Mitarbeitern treffen.</indent>"
    m_Metadata:
      m_Items: []
  - m_Id: 2620668432306176
    m_Localized: "-<indent=5%>Seien Sie sich Ihres Einflusses auf das Arbeitsumfeld
      bewusst und vermeiden Sie G\xFCnstlingswirtschaft.</indent>"
    m_Metadata:
      m_Items: []
  - m_Id: 2620709704257536
    m_Localized: "-<indent=5%>Seien Sie vorsichtig, wenn Sie mit Kollegen \xFCber
      soziale Medien und Textnachrichten kommunizieren.</indent>"
    m_Metadata:
      m_Items: []
  - m_Id: 2620743099305984
    m_Localized: "-<indent=5%>Missbrauchen Sie Ihre Autorit\xE4t nicht.</indent>"
    m_Metadata:
      m_Items: []
  - m_Id: 2620798967435264
    m_Localized: "<b>Erinnerung</b>\nWenn Sie Fragen zu diesen Punkten haben, pr\xFCfen
      Sie bitte Ihre lokalen Richtlinien und lassen Sie sich beraten."
    m_Metadata:
      m_Items: []
  - m_Id: 4631539316781056
    m_Localized: "Erkl\xE4ren Sie, dass dem nachgegangen wird."
    m_Metadata:
      m_Items: []
  - m_Id: 4633439663648768
    m_Localized: Versprechen Sie keine Vertraulichkeit und machen Sie keine Versprechungen
    m_Metadata:
      m_Items: []
  - m_Id: 4633509599473664
    m_Localized: Melden Sie sich sofort, lassen Sie sich beraten und seien Sie diskret.
    m_Metadata:
      m_Items: []
  - m_Id: 4633596132159488
    m_Localized: Sagen Sie Ihre eigene Meinung zu der Situation, um Empathie zu zeigen.
    m_Metadata:
      m_Items: []
  - m_Id: 4633979759980544
    m_Localized: "Die Festlegung von Erwartungen w\xE4hrend des Berichtsprozesses
      ist entscheidend."
    m_Metadata:
      m_Items: []
  - m_Id: 4634832814305280
    m_Localized: "Im Falle einer Untersuchung werden alle Ma\xDFnahmen zur Wahrung
      der Vertraulichkeit getroffen, doch k\xF6nnen den Beteiligten Einzelheiten
      mitgeteilt werden."
    m_Metadata:
      m_Items: []
  - m_Id: 4635007720976384
    m_Localized: "Was als N\xE4chstes geschieht, h\xE4ngt vom Vorwurf des unangemessenen
      Verhaltens sowie von den Richtlinien und Verfahren an Ihrem Arbeitsplatz ab."
    m_Metadata:
      m_Items: []
  - m_Id: 4635145419976704
    m_Localized: "Dies ist keine korrekte Antwort, da die Abgabe einer Meinung den
      Prozess verzerren und die Objektivit\xE4t untergraben kann."
    m_Metadata:
      m_Items: []
  - m_Id: 5044474736373760
    m_Localized: "Was Sie sagen oder tun, oder was nicht gesagt oder getan wird,
      hat oft gro\xDFe Auswirkungen.\u202F\u202F\_\n\_\nSorgen Sie daf\xFCr, dass
      sich die Person nicht von ihren Kollegen isoliert oder von ihren Arbeitsm\xF6glichkeiten
      und Verantwortlichkeiten getrennt f\xFChlt."
    m_Metadata:
      m_Items: []
  - m_Id: 5377724755308544
    m_Localized: "\xC4u\xDFern Sie sich offen"
    m_Metadata:
      m_Items: []
  - m_Id: 5377927554101248
    m_Localized: Es kann manchmal schwierig sein, zu entscheiden, ob man etwas sagen
      soll, wenn man eine angespannte Situation am Arbeitsplatz erlebt.
    m_Metadata:
      m_Items: []
  - m_Id: 5378092260225024
    m_Localized: "Als Manager:in sollten Sie sich dazu \xE4u\xDFern und auch andere
      ermutigen, sich zu \xE4u\xDFern. Sie sind ein Vorbild f\xFCr ein Verhalten,
      das mit den Werten von McDonald's \xFCbereinstimmt. \_"
    m_Metadata:
      m_Items: []
  - m_Id: 5479293621964800
    m_Localized: <b>"Solche Kommentare sind am Arbeitsplatz unangebracht."</b>
    m_Metadata:
      m_Items: []
  - m_Id: 5479429462888448
    m_Localized: "<b>\u201EDiese Art von Kommentaren k\xF6nnte jemanden beleidigen
      und sie sind hier unangebracht.\u201C Kommen wir zur\xFCck zum Thema.\u201C</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 5479531518693376
    m_Localized: "<b>\"Ich muss Sie darauf hinweisen, dass die Bemerkung, die Sie
      vorhin gemacht haben, jemanden beleidigen k\xF6nnte und nicht mit unseren Werten
      \xFCbereinstimmt.</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 5479641749196800
    m_Localized: "<b>\u201EIch brauche Rat dazu, wie ich mit etwas umgehen soll,
      das heute passiert ist.\u201C</b>"
    m_Metadata:
      m_Items: []
  - m_Id: 5727196316266496
    m_Localized: <b>A</b>
    m_Metadata:
      m_Items: []
  - m_Id: 5727260262625280
    m_Localized: <b>B</b>
    m_Metadata:
      m_Items: []
  - m_Id: 5727327660896256
    m_Localized: <b>C</b>
    m_Metadata:
      m_Items: []
  - m_Id: 5727378479083520
    m_Localized: <b>D</b>
    m_Metadata:
      m_Items: []
  - m_Id: 5735202403778560
    m_Localized: "Beispiele f\xFCr Missbrauch von Autorit\xE4t:"
    m_Metadata:
      m_Items: []
  - m_Id: 5735498689413120
    m_Localized: Karte umdrehen, um weitere Informationen zu erhalten
    m_Metadata:
      m_Items: []
  - m_Id: 6563320299212800
    m_Localized: "Du wei\xDFt, dass es mir nicht gef\xE4llt, wie sie dich behandeln,
      aber lass uns\nehrlich sein: Was hast du erwartet? \nDass sie eine Party f\xFCr
      dich schmei\xDFen wollten?"
    m_Metadata:
      m_Items: []
  - m_Id: 6563519709007872
    m_Localized: "Mir gef\xE4llt diese Situation auch nicht, aber ich hatte keine
      Wahl.\nDu wei\xDFt, dass ich Miguel nicht einmal einplanen kann, \nbis die
      Untersuchung abgeschlossen ist. Aber was mir Sorgen macht, ist\ndass niemand
      redet, die Arbeit leidet, und ich kann das \nin meinem Restaurant nicht haben."
    m_Metadata:
      m_Items: []
  - m_Id: 6563633269788672
    m_Localized: "Ich sage nicht, dass es so ist. Aber ich habe gute Neuigkeiten,
      es gibt eine freie Stelle\nIm Restaurant in der Washington Ave. Es ist n\xE4her
      an Ihrem Haus, richtig?"
    m_Metadata:
      m_Items: []
  - m_Id: 6563646171467776
    m_Localized: Raten Sie mal, wer Ihnen eine Versetzung verschafft hat? Sie brauchen
      mir nicht zu danken.
    m_Metadata:
      m_Items: []
  - m_Id: 6564142617677824
    m_Localized: Lustig.
    m_Metadata:
      m_Items: []
  - m_Id: 6564282116034560
    m_Localized: Aber das ist nicht meine Schuld!
    m_Metadata:
      m_Items: []
  - m_Id: 6564307944558592
    m_Localized: Hm, hm...
    m_Metadata:
      m_Items: []
  - m_Id: 6564322591068160
    m_Localized: "\u2026"
    m_Metadata:
      m_Items: []
  - m_Id: 7333489694953472
    m_Localized: 
    m_Metadata:
      m_Items: []
  - m_Id: 7603158175666176
    m_Localized: "Als F\xFChrungskraft ist es Ihre Aufgabe, f\xFCr die Einhaltung
      von Standards und ein sicheres Arbeitsumfeld zu sorgen. Sie werden st\xE4ndig
      Coaching und Feedback geben m\xFCssen. Dies sollte stets auf faire und respektvolle
      Weise geschehen."
    m_Metadata:
      m_Items: []
  - m_Id: 8375492478193664
    m_Localized: Versprechen Sie keine Vertraulichkeit und machen Sie keine Versprechungen
    m_Metadata:
      m_Items: []
  - m_Id: 8375577408655360
    m_Localized: Melden Sie sich sofort, lassen Sie sich beraten und seien Sie diskret.
    m_Metadata:
      m_Items: []
  - m_Id: 8376570875695104
    m_Localized: "Schlagen Sie der Person vor, ein paar Tage zu warten, um zu sehen,
      ob sich das Problem von selbst l\xF6st."
    m_Metadata:
      m_Items: []
  - m_Id: 8376643999191040
    m_Localized: Sagen Sie Ihre eigene Meinung zu der Situation, um Empathie zu zeigen.
    m_Metadata:
      m_Items: []
  - m_Id: 8377561062789120
    m_Localized: "Im Falle einer Untersuchung werden alle Ma\xDFnahmen ergriffen,
      um f\xFCr die betroffenen Person oder Personen die Vertraulichkeit sicherzustellen.
      M\xF6glicherweise sind einige Details erforderlich, um die Meldung zu untersuchen."
    m_Metadata:
      m_Items: []
  - m_Id: 8377673197506560
    m_Localized: "Was als N\xE4chstes geschieht, h\xE4ngt vom Vorwurf des unangemessenen
      Verhaltens sowie von den Richtlinien und Verfahren an Ihrem Arbeitsplatz ab."
    m_Metadata:
      m_Items: []
  - m_Id: 8377809847930880
    m_Localized: "Dies ist keine korrekte Antwort, denn es ist wichtig, Beschwerden
      umgehend zu bearbeiten und nicht zu verz\xF6gern."
    m_Metadata:
      m_Items: []
  - m_Id: 8377869428019200
    m_Localized: "Dies ist keine korrekte Antwort, da die Abgabe einer Meinung den
      Prozess verzerren und die Objektivit\xE4t untergraben kann."
    m_Metadata:
      m_Items: []
  - m_Id: 11672892315750400
    m_Localized: "Das f\xFChlt sich nicht ganz richtig an."
    m_Metadata:
      m_Items: []
  - m_Id: 11684172002971648
    m_Localized: "Ich sollte das noch einmal \xFCberdenken."
    m_Metadata:
      m_Items: []
  - m_Id: 12024615286820864
    m_Localized: "@McDonald\u2019s Social Media Interactions with Co-Workers"
    m_Metadata:
      m_Items: []
  - m_Id: 15218309350809600
    m_Localized: "Eine Meinung abzugeben, kann dazu f\xFChren, dass sich der:die
      Mitarbeiter:in wohler f\xFChlt."
    m_Metadata:
      m_Items: []
  - m_Id: 15219142033399808
    m_Localized: "Diese Situationen l\xF6sen sich oft nach einer Abk\xFChlungsphase
      auf."
    m_Metadata:
      m_Items: []
